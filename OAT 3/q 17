using System;

class Program
{
    static void Main()
    {
        Console.Write("Digite um nÃºmero inteiro:");
        int numero = Convert.ToInt32(Console.ReadLine());

        if (numero >= 0 && numero <= 9)
        {
            Console.WriteLine("Valor valido");
        }
        else
        {
            Console.WriteLine("Valor invalido");
        }
    }
}
