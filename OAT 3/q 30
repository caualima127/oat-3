using System;

class Program
{
    static void Main()
    {
        Console.Write("Digite o n√∫mero da tabuada que voce quer:");
        int numero = Convert.ToInt32(Console.ReadLine());

        Console.WriteLine("Tabuada do {numero}:");

        for (int i = 1; i <= 10; i++)
        {
            Console.WriteLine($"{numero} x {i} = {numero * i}");
        }
    }
}

