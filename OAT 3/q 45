using System;

class Program
{
    static void Main()
    {
        Console.Write("Digite a quantidade de numeros na sequencia: ");
        int n = Convert.ToInt32(Console.ReadLine());

        int[] vetor = new int[n];

        Console.WriteLine("Digite os numeros da sequencia:");

        for (int i = 0; i < n; i++)
        {
            Console.Write("Numero {i + 1}: ");
            vetor[i] = Convert.ToInt32(Console.ReadLine());
        }

        Console.WriteLine("Sequencia na ordem inversa:");

        for (int i = n - 1; i >= 0; i--)
        {
            Console.WriteLine(vetor[i]);
        }
    }
}
